{
    "name": "hichemtab-tech/laravel-project-assist",
    "description": "Laravel-ProjectAssist is a comprehensive toolkit for Laravel developers. It includes custom console commands, service providers, and utility classes that enhance productivity and simplify common tasks in Laravel project development. With thorough documentation and examples, this toolkit streamlines setup, reduces boilerplate code, and improves overall efficiency.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "HichemtabTech\\LaravelProjectAssist\\": "src/",
            "App\\Repositories\\": "src/Repositories/",
            "App\\Console\\": "src/Console/"
        }
    },
    "authors": [
        {
            "name": "HichemTab",
            "email": "konanhichemsinshi@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "HichemtabTech\\LaravelProjectAssist\\ProjectAssistServiceProvider"
            ],
            "commands": [
                {
                    "name": "MakeRepository",
                    "path": "src/Console/Commands/MakeRepository.php"
                },
                {
                    "name": "MakeTrait",
                    "path": "src/Console/Commands/MakeTrait.php"
                },
                {
                    "name": "MakeDataClass",
                    "path": "src/Console/Commands/MakeDataClass.php"
                },
                {
                    "name": "MakeLangFile",
                    "path": "src/Console/Commands/MakeLangFile.php"
                }
            ]
        }
    }
}
